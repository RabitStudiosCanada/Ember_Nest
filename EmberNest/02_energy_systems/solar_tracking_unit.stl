solid embernest_solar_array_mount

  // Base Plate
  facet normal 0 0 1
    outer loop
      vertex 0 0 0
      vertex 100 0 0
      vertex 0 50 0
    endloop
  endfacet

  facet normal 0 0 1
    outer loop
      vertex 100 0 0
      vertex 100 50 0
      vertex 0 50 0
    endloop
  endfacet

  // Vertical Tilt Support Arm
  facet normal 1 0 0
    outer loop
      vertex 100 0 0
      vertex 100 0 40
      vertex 100 50 40
    endloop
  endfacet

  facet normal 1 0 0
    outer loop
      vertex 100 50 0
      vertex 100 50 40
      vertex 100 0 0
    endloop
  endfacet

  // Solar Panel Bed (Tiltable Plane)
  facet normal 0 0 1
    outer loop
      vertex 100 0 40
      vertex 160 0 40
      vertex 100 50 40
    endloop
  endfacet

  facet normal 0 0 1
    outer loop
      vertex 160 0 40
      vertex 160 50 40
      vertex 100 50 40
    endloop
  endfacet

  // Servo Bracket Pocket (Rear Mount)
  facet normal 0 0 -1
    outer loop
      vertex 95 25 10
      vertex 95 25 30
      vertex 100 25 10
    endloop
  endfacet

  facet normal 0 0 -1
    outer loop
      vertex 100 25 10
      vertex 100 25 30
      vertex 95 25 30
    endloop
  endfacet

endsolid
